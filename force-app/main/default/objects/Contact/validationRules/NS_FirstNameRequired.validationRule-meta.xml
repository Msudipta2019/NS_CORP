<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fullName>NS_FirstNameRequired</fullName>
    <active>true</active>
    <description>Requires the user to populate First Name field before saving a Contact record</description>
    <errorConditionFormula>AND(
 ISBLANK( FirstName ),
 RecordType.DeveloperName=&#39;NS_GeneralContact&#39;
)</errorConditionFormula>
    <errorMessage>Please enter First Name.</errorMessage>
</ValidationRule>
