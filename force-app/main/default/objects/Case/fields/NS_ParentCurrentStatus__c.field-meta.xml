<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NS_ParentCurrentStatus__c</fullName>
    <externalId>false</externalId>
    <formula>CASE(Text( Parent.Status ),
&quot;NS_Closed&quot;, &quot;Closed&quot;, 
&quot;NS_Escalated&quot;, &quot;Escalated&quot;,
&quot;NS_New&quot;,&quot;New&quot;,
&quot;NS_Working&quot;,&quot;Working&quot;,
&quot;NS_AwaitingResponse&quot;,&quot;Awaiting Response&quot;,
&quot;NS_Cancelled&quot;,&quot;Cancelled&quot;,
&quot;NS_Reopened&quot;,&quot;Reopened&quot;,
Text( Parent.Status ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Parent Case Current Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
