<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NS_CaseAge__c</fullName>
    <description>It is the number of days  a Case is in opened status</description>
    <externalId>false</externalId>
    <formula>IF(IsClosed,(ClosedDate-CreatedDate),(now()- CreatedDate))</formula>
    <inlineHelpText>What is the total number of days that a Case has been open?</inlineHelpText>
    <label>Total Days Open</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
