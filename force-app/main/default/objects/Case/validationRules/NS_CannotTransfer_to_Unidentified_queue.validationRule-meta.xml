<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NS_CannotTransfer_to_Unidentified_queue</fullName>
    <active>true</active>
    <description>CS – Unidentified queue cannot be set as case owner</description>
    <errorConditionFormula>AND(ISCHANGED( OwnerId ),Owner:Queue.DeveloperName = &#39;CS_Unidentified&#39;, $Profile.Name &lt;&gt; &#39;System Administrator&#39;,
NOT(ISCHANGED(NS_Last_Activity_Time__c)),
NOT(ISCHANGED(NS_Counter_Activity__c))
)</errorConditionFormula>
    <errorMessage>CS – Unidentified queue cannot be set as case owner</errorMessage>
</ValidationRule>
